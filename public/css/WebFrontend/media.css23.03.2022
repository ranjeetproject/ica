        padding: 8px 35px;
    }

    .navbar-toggler {
        display: block;
    }

    .header-bottom .navbar {
        padding: 0;
    }

    .header-bottom .navbar-collapse {
        padding: 20px 0;
    }

    .banner-wrp.db-banner {
        padding-top: 59%;
        background-size: 100%;
    }

    .db-block1 .col-md-4 {
        width: 49%;
    }

    .progress-wrap .col-md-6 {
        width: 100%;
        margin: 20px 0;
    }

    .course-card.db-cards {
        margin-bottom: 40px;
    }

    section.banner-wrp.inner-banner {
        background-position: 92% top;
        padding-top: 75%;
        background-size: 262%;
    }
  
}

