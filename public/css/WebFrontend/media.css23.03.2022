    }
}

@media screen and (max-width: 991px) {
    .crs-dtls-wrp .contents .sub-para {
        font-size: 18px;
        line-height: 32px;
        margin-bottom: 25px;
    }
    .banner-wrp {
        background-position: 80% top;
        padding-top: 74%;
        background-size: 150%;
    }

    .ban-content::after {
        display: none;
    }

    .ban-content {
        max-width: 100%;
    }

    .how-it-works .h-header-wrp {
        width: 100%;
    }

    .footer-wrapper .footer-logo-wrap,
    .footer-wrapper .footer-quick-links,
    .footer-wrapper .footer-so-li,
    .footer-wrapper .footer-address {
        width: 100%;
        text-align: center;
        margin: 20px 0;
    }

    .footer-wrapper .add-bar .label {
        flex: inherit;
    }

    .footer-wrapper .add-bar {
        justify-content: center;
    }

    .footer-wrapper .text-prt {
        font-size: 16px;
    }

    .footer-wrapper .btn-about {
